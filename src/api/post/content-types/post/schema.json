{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "imageFeatured": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "string"
    },
    "type": {
      "type": "customField",
      "default": "[]",
      "options": [
        "news",
        "announcements",
        "tutorials"
      ],
      "customField": "plugin::multi-select.multi-select"
    }
  }
}
